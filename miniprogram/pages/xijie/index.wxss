@font-face {
    font-family: 'iconfont';
    src: url('data:font/woff2;charset=utf-8;base64,') format('woff2'),
        url('data:font/woff;charset=utf-8;base64,') format('woff'),
        url('iconfont.ttf') format('truetype');
    font-weight: normal;
    font-style: normal;
    font-display: swap;
}
.iconfont {
    font-family: "iconfont" !important;
    font-size: 16px;
    font-style: normal;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
  }
  
  .icon-user:before {
    content: "\e60e";
  }
view {
    padding: 0;
    margin: 0;
    box-sizing: border-box;
}
.free_time,
.ticket
{
    width: 100%;
    height: 80rpx;
    padding-left: 30rpx;
    padding-top:5rpx;
    border-bottom: 2rpx solid #ccc;
}

.free_time_title,
.ticket_title,
.title {
    display: inline-block;
    font-size: 48rpx;
    font-weight: 800;
    margin-right: 20rpx;
    text-align: center;
    margin-bottom: 20rpx;
}
.free_time_state,
.ticket_state,
.spotline ul li text,
.spotline ul li view {
    font-size: 32rpx;
}
.spotline {
    width: 100%;
    height: 900rpx;
    padding: 25rpx;
    border-bottom: 2rpx solid #cfcfcf;
}
.spotline ul li view{
    padding: 20rpx;
}
.spotline ul li text{
    display: inline-block;
    width: 100%;
    padding: 20rpx;
    border-bottom: 1rpx solid #ccc;
}
.spotline ul li .last {
    border-bottom: none;
}
.chuxin {
    display: inline-block;
    height: 30rpx;
    width: 100%;
    text-align: center;
    color: #ccc;
}
.lunbo {
    border-radius: 20rpx;
    overflow: hidden;
    width: 95%;
    height: 400rpx;
    margin: 10rpx auto;
  }
  .lunbo swiper-item image {
      width: 100%;
  }
   .pingjia {
       width: 100%;
       display: flex;
       margin-top: 15rpx;
     padding: 0 20rpx;
  } 
  .words {
      flex: 9;
      font-size: 32rpx;
      letter-spacing: 3rpx;
      /* border-bottom: 1rpx solid #ccc; */
}
.judge_time {
    color: #ccc;
    font-size: 25rpx;
    letter-spacing: 2rpx;
}
  .name {
      display: inline-block;
      flex: 1;
      margin-right: 10rpx;
      font-family: 'icomoon';
      font-size: 50rpx;
      width: 75rpx;
      height: 30rpx;
      border-radius: 10rpx;
      margin-top: 20rpx;
      /* background-color: rgb(240, 68, 68); */
      color: red;
      text-align: center;
      line-height: 30rpx;
  }
  .name text {
      margin-top: 20rpx;
      display: block;
      font-size: 25rpx;
  }
